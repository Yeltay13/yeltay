---
import Navbar from '../components/Navbar.astro';
import Hero from '../components/Hero.astro';
import Features from '../components/Features.astro';
import Testimonials from '../components/Testimonials.astro';
import Pricing from '../components/Pricing.astro';
import Footer from '../components/Footer.astro';
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Yeltay</title>
    <link rel="stylesheet" href="/styles/tailwind.css">
    <meta property="og:title" content="Yeltay">
    <meta property="og:description" content="Mentor in Tech Soft Skills and Career Growth.">
    <meta property="og:image" content="https://yeltay.com/my-photo.jpg">
    <meta property="og:url" content="https://yeltay.com/">
    <meta property="og:type" content="website">
    <style>
      html {
        scroll-behavior: smooth;
      }
    </style>
  </head>
  <body>
    <Navbar />
    <Hero />
    <Features />
	  <Testimonials />
    <Pricing />
    <Footer />
  

  </body>
</html>
